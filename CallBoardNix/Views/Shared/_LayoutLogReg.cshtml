<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Register Page</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/CallBoardNix.styles.css" asp-append-version="true" />
</head>
<body style="background-color:#e8f0fc">
    <header style="background-color:#0d6efd">
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm border-bottom border-primary box-shadow mb-0">
            <div class="container">
               <a class="navbar-brand w-25" style="color:#ffc107" asp-controller="Home" asp-action="Index">CallBoardNix</a>
            </div>
        </nav>
    </header>
    <div>
        @RenderBody()
    </div>
    <footer class="border-top border-primary footer text-muted" style="background-color:#0d6efd">
    <div class="container" style="color:#ffc107">
        &copy; 2022 - CallBoardNix
    </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>

</html>
